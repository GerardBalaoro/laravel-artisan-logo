{
    "name": "gerardbalaoro/laravel-artisan-logo",
    "description": "Display app FIGlet logo on artisan commands",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "gerardbalaoro/figlet-php": "^1.0",
        "illuminate/console": "^5.6|^6",
        "illuminate/support": "^5.6|^6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gerard Balaoro",
            "email": "gmbalaoro@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArtisanLogo\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ArtisanLogo\\ServiceProvider"
            ]
        }
    }
}
